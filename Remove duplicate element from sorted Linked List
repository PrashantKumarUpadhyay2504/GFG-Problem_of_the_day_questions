class GfG
{
    //Function to remove duplicates from sorted linked list.
    Node removeDuplicates(Node head)
    {Node temp = head;
        if(temp.next == null) return head;
        while(temp.next.next != null){
            if(temp.data == temp.next.data) 
                temp.next = temp.next.next;
            else
                temp = temp.next;
        }
        
        if(temp.next != null){
            if(temp.data == temp.next.data)
                temp.next = null;
        }
        
        return head;
	// Your code here	
    }
}
